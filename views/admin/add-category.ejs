<script>
    let b64DecodeUnicode3 = str => decodeURIComponent(Array.prototype.map.call(atob(str), c => '%' + ('00' + c.charCodeAt(0).toString(16)).slice(-2)).join(''))
    let parseJwt3 = token => JSON.parse(b64DecodeUnicode3( token.split('.')[1].replace('-', '+').replace('_', '/') ))

    const as3245 = sessionStorage.getItem("a456b")
    const userData = as3245 == null ? null : parseJwt(as3245)

    if (userData == null) {
        window.location.href = "/account"
    }
    if (userData?.role != "admin") {
        window.location.href = "/products"
    }

</script>

